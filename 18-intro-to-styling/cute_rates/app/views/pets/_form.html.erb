<%= form_for(@pet) do |f| %>
  <%= f.label "Name" %>
  <%= f.text_field :name %>
  <%= f.label "Image" %>
  <%= f.text_field :image %>
  <%= f.label "Likes" %>
  <%= f.number_field :likes %>
  <%= f.label "Owner" %>
  <%= f.collection_select(:person_id, @people, :id, :name) %>
  <%= f.submit %>
<% end %>
